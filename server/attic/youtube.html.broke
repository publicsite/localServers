<HTML>
<HEAD>
<script type="text/javascript">

function xmlHTMLGet(strURL) {
alert(strURL);
var newurl = '';
    var xmlHttpReq = false;
    var self = this;
    if (window.XMLHttpRequest) { // Mozilla/Safari
        self.xmlHttpReq = new XMLHttpRequest();
    }
    else if (window.ActiveXObject) { // IE
        self.xmlHttpReq = new ActiveXObject("Microsoft.XMLHTTP");
    }
    self.xmlHttpReq.open('GET', newurl + strURL, false);
    self.xmlHttpReq.onload = function() {
        if (self.xmlHttpReq.status == 200) {
    var xmlDoc = self.xmlHttpReq.responseXML;
theHTML="<html><head><base target=\"_blank\"></head><center>" + xmlDoc + "</center></html>"
window.frames[0].document.body.innerHTML = theHTML;

            //document.getElementById("theFrame").document.body.innerHTML = theText2;
        }
    }

    self.xmlHttpReq.send();
}

function searchFunction(){
theurl="http://127.0.0.1:8082/youtube.com/results?search_query=" + document.getElementById("thetextbox").value();
xmlHTMLGet(theurl);
}

</script>
</HEAD>
